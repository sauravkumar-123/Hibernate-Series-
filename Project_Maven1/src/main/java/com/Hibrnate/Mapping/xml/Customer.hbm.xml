<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 


<hibernate-mapping>
   <class name = "com.Hibrnate.Mapping.xml.Customer" table = "customer">
   
   <meta attribute = "class-description">
         This class contains the Customer detail. 
      </meta>
      
    <id name = "cust_id" type = "int" column = "customer_id"> 
    <generator class="native"></generator>
    </id>
   <property name = "cust_name" column = "Customer_Name" type = "string"/>
   <property name = "gender" column = "Customer_gender" type = "string"/>
   <property name = "email" column = "Customer_email" type = "string"/>
   <property name = "mobno" column = "Customer_Mobno" type = "string"/>
   </class>

</hibernate-mapping>